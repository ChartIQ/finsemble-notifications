{
  "name": "finsemble-notifications",
  "version": "0.0.1",
  "description": "Notifications services and UI",
  "scripts": {
    "watch": "nodemon -e js,ts,txs,jsx,html,css,json copy.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode=development --config=./webpack.config.js",
    "build:dev": "webpack --mode=development --watch",
    "prestart": "npm run build",
    "start": "webpack-dev-server --mode=development --config=./webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChartIQ/finsemble-notifications.git"
  },
  "author": "ChartIQ",
  "bugs": {
    "url": "https://github.com/ChartIQ/finsemble-notifications/issues"
  },
  "homepage": "https://github.com/ChartIQ/finsemble-notifications#readme",
  "dependencies": {
    "@chartiq/finsemble": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.5",
    "fs-extra": "^8.1.0",
    "nodemon": "^2.0.0",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@chartiq/tslint-config-chartiq-base": "^0.1.0",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "rimraf": "^3.0.2",
    "style-loader": "^0.19.1",
    "terser-webpack-plugin": "^1.4.1",
    "url-join": "^2.0.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.2",
    "webpack-glob-entries": "^1.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
